<Window x:Class="OEIKnapperGUI.ProjectSelectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OEIKnapperGUI"
        mc:Ignorable="d"
        Title="Select game" Height="600" Width="450">
    
    <Window.Resources>
        <DataTemplate x:Key="projectItemTemplate">
            <StackPanel Orientation="Horizontal">
                <Image x:Name="gameIcon"></Image>
                <TextBlock x:Name="gameName"></TextBlock>
                <TextBlock x:Name="gamePath"></TextBlock>
            </StackPanel>
        </DataTemplate>
    </Window.Resources>
    <Grid>
    
    </Grid>
</Window>
